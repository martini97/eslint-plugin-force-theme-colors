{
  "name": "eslint-plugin-force-theme-colors",
  "version": "0.0.0",
  "description": "Disallow defining colors outside the styled theme",
  "main": "./dist/index.js",
  "author": "Alessandro Martini <github.com/martini97>",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/martini97/eslint-plugin-force-theme-colors",
  "devDependencies": {
    "@types/eslint": "^7.2.13",
    "@types/estree": "^0.0.48",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "resolve-from": "^5.0.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tsup": "^4.11.2",
    "typescript": "^4.3.2"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --no-splitting --minify",
    "commit": "cz",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
